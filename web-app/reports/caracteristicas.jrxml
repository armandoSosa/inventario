	<subDataset name="caracteristicas" uuid="0719e7ef-47f5-4053-b743-883732c561ba">
		<parameter name="IDOBJETO" class="java.lang.String">
			<defaultValueExpression><![CDATA[4]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
     CARACTERISTICA."CARACTERISTICA" AS CARACTERISTICA_CARACTERISTICA,
     VALOR."VALOR" AS VALOR_VALOR,
     UNIDAD."UNIDAD" AS UNIDAD_UNIDAD
FROM
     "PUBLIC"."OBJETO" OBJETO INNER JOIN "PUBLIC"."TIPO" TIPO ON OBJETO."TIPO_ID" = TIPO."ID"
     INNER JOIN "PUBLIC"."TIPO_PROPIEDAD" TIPO_PROPIEDAD ON OBJETO."TIPO_PROPIEDAD_ID" = TIPO_PROPIEDAD."ID"
     INNER JOIN "PUBLIC"."VALOR" VALOR ON OBJETO."ID" = VALOR."OBJETOS_ID"
     INNER JOIN "PUBLIC"."PLANTILLA" PLANTILLA ON VALOR."PLANTILLA_ID" = PLANTILLA."ID"
     AND TIPO."ID" = PLANTILLA."TIPO_ID"
     INNER JOIN "PUBLIC"."CARACTERISTICA_UNIDAD" CARACTERISTICA_UNIDAD ON PLANTILLA."CARACTERISTICA_UNIDAD_ID" = CARACTERISTICA_UNIDAD."ID"
     INNER JOIN "PUBLIC"."CARACTERISTICA" CARACTERISTICA ON CARACTERISTICA_UNIDAD."CARACTERISTICA_ID" = CARACTERISTICA."ID"
     INNER JOIN "PUBLIC"."UNIDAD" UNIDAD ON CARACTERISTICA_UNIDAD."UNIDAD_ID" = UNIDAD."ID"
WHERE
     OBJETO.ID = $P{IDOBJETO}]]>
		</queryString>
		<field name="CARACTERISTICA_CARACTERISTICA" class="java.lang.String"/>
		<field name="VALOR_VALOR" class="java.lang.String"/>
		<field name="UNIDAD_UNIDAD" class="java.lang.String"/>
	</subDataset>
	<parameter name="IDOBJETO" class="java.lang.String">
		<defaultValueExpression><![CDATA[4]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     OBJETO."NO_INVENTARIO" AS OBJETO_NO_INVENTARIO,
     TIPO_PROPIEDAD."NOMBRE" AS TIPO_PROPIEDAD_NOMBRE,
     TIPO."DESCRIPCION" AS TIPO_DESCRIPCION,
     CARACTERISTICA."CARACTERISTICA" AS CARACTERISTICA_CARACTERISTICA,
     VALOR."VALOR" AS VALOR_VALOR,
     PLANTILLA."ORDEN" AS PLANTILLA_ORDEN
FROM
     "PUBLIC"."OBJETO" OBJETO INNER JOIN "PUBLIC"."TIPO" TIPO ON OBJETO."TIPO_ID" = TIPO."ID"
     INNER JOIN "PUBLIC"."TIPO_PROPIEDAD" TIPO_PROPIEDAD ON OBJETO."TIPO_PROPIEDAD_ID" = TIPO_PROPIEDAD."ID"
     INNER JOIN "PUBLIC"."VALOR" VALOR ON OBJETO."ID" = VALOR."OBJETOS_ID"
     INNER JOIN "PUBLIC"."PLANTILLA" PLANTILLA ON VALOR."PLANTILLA_ID" = PLANTILLA."ID"
     AND TIPO."ID" = PLANTILLA."TIPO_ID"
     INNER JOIN "PUBLIC"."CARACTERISTICA_UNIDAD" CARACTERISTICA_UNIDAD ON PLANTILLA."CARACTERISTICA_UNIDAD_ID" = CARACTERISTICA_UNIDAD."ID"
     INNER JOIN "PUBLIC"."CARACTERISTICA" CARACTERISTICA ON CARACTERISTICA_UNIDAD."CARACTERISTICA_ID" = CARACTERISTICA."ID"
WHERE
     OBJETO.ID = $P{IDOBJETO}]]>
	</queryString>
	<field name="OBJETO_NO_INVENTARIO" class="java.lang.String"/>
