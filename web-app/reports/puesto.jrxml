	<queryString>
		<![CDATA[SELECT
     DEPARTAMENTO."NOMBRE" AS DEPARTAMENTO_NOMBRE,
     PUESTO."NOMBRE" AS PUESTO_NOMBRE
FROM
     "PUBLIC"."PERSONA" PERSONA INNER JOIN "PUBLIC"."PUESTO_PERSONA" PUESTO_PERSONA ON PERSONA."ID" = PUESTO_PERSONA."PERSONA_ID"
     INNER JOIN "PUBLIC"."PUESTO" PUESTO ON PUESTO_PERSONA."PUESTO_ID" = PUESTO."ID"
     INNER JOIN "PUBLIC"."DEPARTAMENTO" DEPARTAMENTO ON PUESTO."DEPARTAMENTO_ID" = DEPARTAMENTO."ID"
WHERE
     PERSONA.ID = $P{IDPERSONA}
 AND PUESTO_PERSONA.FECHA_FIN IS NULL]]>
	</queryString>
