	<parameter name="IDPERSONA" class="java.lang.String">
		<defaultValueExpression><![CDATA[1]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     DIRECCION."CALLE" AS DIRECCION_CALLE,
     DIRECCION."COLONIA" AS DIRECCION_COLONIA,
     DIRECCION."FECHA" AS DIRECCION_FECHA,
     DIRECCION."NO_EXTERIOR" AS DIRECCION_NO_EXTERIOR,
     DIRECCION."NO_INTERIOR" AS DIRECCION_NO_INTERIOR,
     MUNICIPIO."NOMBRE" AS MUNICIPIO_NOMBRE,
     ESTADO."NOMBRE" AS ESTADO_NOMBRE
FROM
     "PUBLIC"."PERSONA" PERSONA INNER JOIN "PUBLIC"."DIRECCION" DIRECCION ON PERSONA."ID" = DIRECCION."PERSONA_ID"
     INNER JOIN "PUBLIC"."MUNICIPIO" MUNICIPIO ON DIRECCION."MUNICIPIO_ID" = MUNICIPIO."ID"
     INNER JOIN "PUBLIC"."ESTADO" ESTADO ON MUNICIPIO."ESTADO_ID" = ESTADO."ID"
WHERE PERSONA.ID = $P{IDPERSONA}
ORDER BY DIRECCION.FECHA DESC
LIMIT 1]]>
	</queryString>
